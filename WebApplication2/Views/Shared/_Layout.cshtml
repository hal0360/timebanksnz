<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>Timebanks NZ</title>
    <meta name="description" content="" />
    <meta name="keywords" content="time banking, new zealand" />
    <!-- Facebook -->
    <meta property="og:title" content="Timebanks NZ" />
    <meta property="og:type" content="article" />
    <meta property="og:description" content="Timebanks NZ" />
    <meta property="og:image" content="http://www.timebanks.nz/share.jpg" />
    <meta property="og:url" content="http://www.timebanks.nz/" />
    <meta property="og:site_name" content="Timebanks NZ" />
    <meta property="fb:admins" content="" />
    <!-- Twitter -->
    <meta name="twitter:card" content="summary_large_image" />
    
    <meta name="twitter:title" content="Timebanks NZ" />
    <meta name="twitter:description" content="" />
    <meta name="twitter:image:src" content="http://www.timebanks.nz/twitter-card.jpg" />

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body id="anchor-top">

    <div class="navbar navbar-inverse navbar-fixed-top topbar">
        <div class="container topbar">
            <div class="navbar-header topbar">
                <button type="button" class="navbar-toggle okbar" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    
                </button>

            </div>
            <div class="navbar-collapse collapse topbar">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Addington", "dfdf", "dfdf", null, new { style = "color:white" })</li>
                    <li>@Html.ActionLink("Lyttelton", "dd", "dd", null, new { style = "color:white" })</li>
                    <li>@Html.ActionLink("Somewhere", "dd", "dd", null, new { style = "color:white" })</li>
                    <li>@Html.ActionLink("Anywhere", "ff", "ddd", null, new { style = "color:white" })</li>
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>

    <br>
    <br>
    <br>

    <div id="logo-area">
        <div class="container">
            <div class="row">
                <h1 class="col-sm-4 col-sm-offset-1 col-md-4 col-md-offset-2 col-lg-3">
                    <a href="/"><img src="~/img/timebanksnz_logo.png" width="206" height="77" /><span>Timebanks NZ</span></a>
                </h1>
                <div class="bunch-of-buttons col-sm-6 col-sm-offset-1 col-md-4 col-md-offset-0 col-lg-5">
                    
                    <div class="row">
                        <div class="social-media col-sm-12">
                            <ul>
                                <li><a href="#">Facebook</a></li>
                                <li> | </li>
                                <li><a href="#">Twitter</a></li>
                                <li> | </li>
                                <li><a href="#">Google+</a></li>
                                <li> | </li>
                                <li><a href="#">Pinterest</a></li>
                                <li> | </li>
                                <li><a href="#">Email</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <header id="page-header">
        <div class="container">
            <div class="row">
                <div class="global-nav col-sm-11 col-sm-offset-1 col-md-8 col-md-offset-2 col-lg-8 col-lg-offset-2">
                    <ul class="list-inline">
                        <li>@Html.ActionLink("Welcome", "Index", "Home")</li>
                        <li>@Html.ActionLink("Timebanking", "Timebanking", "Home")</li>
                        <li>@Html.ActionLink("Join", "Register", "Account")</li>
                        <li>@Html.ActionLink("Offers", "Offers", "Home")</li>
                        <li>@Html.ActionLink("Requests", "Requests", "Home")</li>
                        <li>@Html.ActionLink("News", "News", "Home")</li>
                    </ul>
                </div>
            </div>
        </div>
    </header>

    <div class="container body-content">


        <script src="http://maps.google.com/maps/api/js?sensor=false&libraries=geometry&v=3.7">
        </script>
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)

        @RenderBody()
  

        <hr />
     
    </div>

    <footer id="page-footer">
        <div class="container">
            <div class="row">
                <div class="footer-block col-sm-11 col-sm-offset-1 col-md-8 col-md-offset-2 col-lg-8 col-lg-offset-2">
                    <ul>
                        <li>@Html.ActionLink("Welcome", "Index", "Home")</li>
                        <li>@Html.ActionLink("Timebanking", "Timebanking", "Home")</li>
                        <li>@Html.ActionLink("Join", "Register", "Account")</li>
                        <li>@Html.ActionLink("Offers", "Offers", "Home")</li>
                        <li>@Html.ActionLink("Requests", "Requests", "Home")</li>
                        <li>@Html.ActionLink("News", "News", "Home")</li>
                        <li><a href="/#">Contact</a></li>
                    </ul>
                </div>
                <div class="footer-copyright col-sm-11 col-sm-offset-1 col-md-8 col-md-offset-2 col-lg-8 col-lg-offset-2">
                    <p>&copy; 2015 Timebanks.nz</p>
                </div>
            </div>
        </div>

    </footer>
</body>
</html>
